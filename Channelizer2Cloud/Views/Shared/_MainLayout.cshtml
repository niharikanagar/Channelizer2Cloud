<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta charset="utf-8">
    <link rel="icon" href="~/Content/img/Channelizer.ico" type="image/x-icon" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/maincss")
    @Scripts.Render("~/bundles/modernizr")
</head>

<body class="s-loginform-main">
    <div class="container-fluid s-container">
        <div class="row">
            <div class="col-md-12 s-center s-login_margintop">
            </div>
        </div>
        @RenderBody()
        <div class="row">
            <div class=" col-md-12 s-center s-bottom-footer">
                Copyright &copy; @DateTime.Now.Year Channelizer2Cloud Inc. All Rights Reserved.
            </div>
        </div>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/javascript")
    @RenderSection("scripts", required: false)
</body>
</html>
